<h2><%=t :find_by_address %></h2>
<div class="form-group">
  <input id="address" type="textbox" class="form-control" placeholder="<%=t :enter_address %>">
</div>
<div class="form-group">
  <input type="button" class="btn btn-info" value="Geocode" onclick="codeAddress()">
</div>
<div class="embed-responsive embed-map">
  <div id="map" class="embed-responsive-item">
  </div>
</div>

<% content_for :footer do %>
<%= javascript_include_tag '//maps.googleapis.com/maps/api/js?key=AIzaSyDkiHUkl-Syk3ZkNZYQcmLQZW3mNQZYj-Y&libraries=places&callback=initMapLocations' %>
<% end %>
