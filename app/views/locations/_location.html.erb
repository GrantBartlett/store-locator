<div class="location-info-box">
  <ul class="list-unstyled">
    <li>
      <h4><%= location.name %></h4>
    </li>
    <li>
      <p>
        <%= location.content %>
      </p>
      <p>
        <%= link_to 'Visit Website', location.url %>
      </p>
    </li>
    <li>
      Lat - <%= location.lat %>
    </li>
    <li>
      Lng - <%= location.lng %>
    </li>
  </ul>
  <button class="btn btn-dark" onclick="openInfoWindow(<%= location_counter %>);">Find shop</button>
</div>
